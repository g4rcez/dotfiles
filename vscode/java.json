{
    "Sysout": {
      "prefix": "sout",
      "body": "\rSystem.out.println(${TM_CURRENT_LINE/(^[\t\r ]+|.?sout$)//gi});",
      "description": "Sysout variables"
    },
    "Main method": {
      "prefix": "psvm",
      "body": ["\tpublic void main(String[] args){", "\t\t$0", "\t}"]
    },
    "Parse double": {
      "body": "Double $1 = Double.parseDouble($0);",
      "prefix": "pad"
    },
    "Parse long": {
      "body": "Long $1 = Long.parseLong($0);",
      "prefix": "pal"
    },
    "Parse string": {
      "body": "String $1 = String.valueOf($0);",
      "prefix": "pas"
    },
    "Class and Constructor": {
      "body": ["public class $TM_FILENAME_BASE{", "\tpublic $TM_FILENAME_BASE(){}", "}"],
      "prefix": "className"
    },
    "Lombok Annotations": {
      "body": ["@Data", "@Builder", "@Accessors(chain = true)"],
      "prefix": "lombok"
    }
  }
