{
    "breadcrumbs.enabled": false,
    "cSpell.language": "en,pt,pt-BR,en-US",
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorStyle": "underline-thin",
    "editor.fontFamily": "Ligconsolata",
    "editor.fontLigatures": true,
    "editor.minimap.enabled": false,
    "editor.parameterHints.cycle": true,
    "editor.scrollBeyondLastLine": false,
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "inline",
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "files.autoSave": "afterDelay",
    "prettier.printWidth": 120,
    "prettier.tabWidth": 4,
    "workbench.colorTheme": "Plastic",
    "workbench.iconTheme": "material-icon-theme",
    "files.watcherExclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules/**": true,
        "**/target/**": true,
    },
    "files.associations": {
        "*.vm": "html"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules/**": true,
        "**/target/**": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "files.autoSaveDelay": 1350,
    "material-icon-theme.activeIconPack": "react_redux",
    "material-icon-theme.hidesExplorerArrows": true,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.useExperimentalGridLayout": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "javascript.implicitProjectConfig.checkJs": true,
    "prettier.arrowParens": "always",
    "prettier.stylelintIntegration": true,
    "prettier.useTabs": true,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "terminal.integrated.rendererType": "dom",
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "prettier.tslintIntegration": true,
    "tslint.suppressWhileTypeErrorsPresent": true,
    "extensions.ignoreRecommendations": false,
    "javascript.preferences.importModuleSpecifier": "non-relative",
    "javascript.preferences.quoteStyle": "double",
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "typescript.preferences.quoteStyle": "double",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "terminal.integrated.lineHeight": 1.5,
    "editor.wordWrapColumn": 120,
    "editor.fontSize": 16,
    "editor.stablePeek": true,
    "editor.tabCompletion": "on",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.tabSizing": "shrink",
    "emmet.triggerExpansionOnTab": true,
    "javascript.referencesCodeLens.enabled": true,
    "typescript.implementationsCodeLens.enabled": true,
    "typescript.referencesCodeLens.enabled": true,
    "java.jdt.ls.vmargs": "-noverify -Xmx1G -XX:+UseG1GC -XX:+UseStringDeduplication -javaagent:\"C:\\Users\\Garcez\\.vscode\\extensions\\gabrielbb.vscode-lombok-0.9.8/server/lombok.jar\"",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.zoomLevel": 0,
    "[css]": {
        "editor.defaultFormatter": "aeschli.vscode-css-formatter"
    },
}