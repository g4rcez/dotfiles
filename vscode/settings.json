{
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "workbench.colorTheme": "One Dark Pro",
    "files.autoSave": "afterDelay",
    "editor.cursorStyle": "underline",
    "editor.wordWrap": "wordWrapColumn",
    "editor.accessibilitySupport": "off",
    "editor.fontLigatures": true,
    "editor.formatOnPaste": true,
    "editor.letterSpacing": 1.1,
    "editor.mouseWheelZoom": true,
    "editor.renderLineHighlight": "gutter",
    "editor.smoothScrolling": true,
    "editor.tabCompletion": true,
    "workbench.commandPalette.preserveInput": true,
    "workbench.editor.labelFormat": "medium",
    "workbench.iconTheme": "vscode-icons",
    "window.menuBarVisibility": "toggle",
    "files.autoSaveDelay": 500,
    "files.insertFinalNewline": true,
    "files.maxMemoryForLargeFilesMB": 8192,
    "html.format.endWithNewline": true,
    "terminal.external.linuxExec": "terminator",
    "autoimport.filesToScan": "**/*.{ts,tsx,js,jsx}",
    "editor.fontFamily": "'Fira Code', 'Inconsolata', monospace, 'Droid Sans Fallback'",
    "workbench.colorCustomizations": {
        "editor.background": "#15191f"
    },
    "editor.minimap.enabled": false,
    "editor.wordWrapColumn": 120,
    "javascript.format.enable": true,
    "eslint.options": {
        "env": {
            "node": true,
            "es6": true,
            "browser": true
        },
        "parserOptions": {
            "ecmaVersion": 6,
            "sourceType": "module",
            "ecmaFeatures": {
                "jsx": true
            }
        },
        "ecmaFeatures":{
            "arrowFunctions": true,
            "blockBindings": true,
            "classes": true,
            "defaultParameters": true,
            "destructuring": true,
            "forOf": true,
            "generators": true,
            "modules": true
        },
        "extends": "eslint:recommended",
    },
    "beautify.config": {
        "brace_style": "collapse",
    },
    "svgviewer.enableautopreview": true,
    "svgviewer.previewcolumn": "Two",
    "npm-intellisense.showBuildInLibs": true,
    "FSharp.fsacRuntime": "netcore",
    "FSharp.lineLens.enabled": "always",
    "FSharp.minimizeBackgroundParsing": true,
    "colorHelper.disableGpu": 0,
    "colorHelper.disableShadow": true,
    "colorHelper.disableTransparent": true,
    "colorHelper.pickerForm": "compact",
    "colorHelper.resident": true,
    "html-css-class-completion.enableEmmetSupport": true,
    "bracketPairColorizer.forceIterationColorCycle": true,
    "bracketPairColorizer.forceUniqueOpeningColor": true,
    "bracketPairColorizer.highlightActiveScope": true,
    "bracketPairColorizer.showBracketsInGutter": true
}
